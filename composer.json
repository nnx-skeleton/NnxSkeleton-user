{
    "name": "nnx-skeleton-member/user",
    "description": "Service User",
    "type": "library",
    "authors": [
        {
            "name": "Plitakov Dmitry",
            "email": "plitakov94@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "doctrine/doctrine-orm-module": "0.*",
        "nnx/doctrine": "0.1.*",
        "nnx/module-options": "0.1.*"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.11.0",
        "phpunit/phpunit": "~4.8.0",
        "phpunit/phpcov": "~2.0.0",
        "satooshi/php-coveralls": "~1.0.0",
        "zendframework/zend-loader": "~2.4.0"
    },

    "autoload": {
        "psr-4": {
            "NnxSkeletonMember\\User\\": "src/"
        },
        "classmap": [
            "./Module.php",
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "NnxSkeletonMember\\User\\PhpUnit\\Test\\": "test/phpunit/tests",
            "NnxSkeletonMember\\User\\PhpUnit\\TestData\\": "test/phpunit/_files"
        },
        "classmap": [
            "./test/phpunit/Bootstrap.php",
            "test/"
        ]
    }
}
